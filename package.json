{
  "private": true,
  "name": "rqg",
  "version": "1.0.0",
  "type": "module",
  "esModuleInterop": true,
  "description": "",
  "scripts": {
    "build": "run-s clean:dist compile",
    "compile": "run-p build:packs copystatic",
    "build:packs": "node --experimental-specifier-resolution=node --loader ts-node/esm -r tsconfig-paths/register buildScripts/buildPacks.ts",
    "copystatic": "copyfiles -f module/module.json dist",
    "compile:noemit": "tsc --noEmit",
    "lint": "eslint ./src --ext .js,.ts --fix",
    "analyze": "run-p compile:noemit lint",
    "test": "jest",
    "test:coverage": "jest --coverage --collectCoverageFrom=./src/**",
    "clean:dist": "rimraf dist",
    "clean:deps": "rimraf node_modules",
    "clean": "run-p clean:dist clean:deps",
    "prepare": "husky install"
  },
  "author": "",
  "license": "UNLICENCED",
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@league-of-foundry-developers/foundry-vtt-types": "0.8.9-2",
    "@snowpack/plugin-sass": "^1.4.0",
    "@snowpack/plugin-typescript": "^1.2.1",
    "@types/fs-extra": "^11.0.1",
    "@types/jest": "^29.2.4",
    "@types/js-yaml": "4.0.5",
    "@types/node": "^18.14.6",
    "@types/socket.io": "^3.0.2",
    "@types/ws": "^8.5.4",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "chalk": "^5.2.0",
    "classic-level": "^1.3.0",
    "copy-files": "^0.1.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "@foundryvtt/foundryvtt-cli": "^0.0.9",
    "fs-extra": "^11.1.0",
    "husky": "^8.0.0",
    "jest": "^29.3.1",
    "js-yaml": "^4.1.0",
    "lint-staged": "^13.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.1",
    "sass": "^1.39.2",
    "snowpack": "^3.8.8",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {}
